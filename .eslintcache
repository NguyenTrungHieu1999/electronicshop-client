totalPrice') || 0;\r\n    const { receiver, receiversAddress, phoneNumber, email, note } = this.props;\r\n    let orderDetailModel = [];\r\n    if (cartItems && cartItems.length) {\r\n      cartItems.map(item => {\r\n        orderDetailModel.push({\r\n          productId: item.product.id,\r\n          quantity: item.total,\r\n          price: item.product.price\r\n        })\r\n      })\r\n    };\r\n    return (\r\n      <PayPalButton\r\n        amount={totalPrice / 20000}\r\n        // shippingPreference=\"NO_SHIPPING\" // default is \"GET_FROM_FILE\"\r\n        onSuccess={(details, data) => {\r\n          // OPTIONAL: Call your server to save the transaction\r\n          if (receiver === '' || receiversAddress === '' || phoneNumber === '' || email === '') {\r\n            alert(\"Qu khch vui lng in y  thng tin.\");\r\n            return null;\r\n          } else {\r\n            return paymentApi\r\n              .checkout({ paid: true, receiver: receiver, receiversAddress: receiversAddress, phoneNumber: phoneNumber, email: email, totalMoney: totalPrice, note: note, orderDetails: orderDetailModel })\r\n              .then(res => {\r\n                if (res.data.isSuccessed) {\r\n                  localStorage.removeItem('cartItems');\r\n                  localStorage.removeItem('totalPrice');\r\n                  alert(\"Thm n hng thnh cng!\");\r\n                  window.location.href = ('/');\r\n                }\r\n              })\r\n              .catch(error => console.log(error))\r\n          }\r\n        }\r\n        }\r\n      />\r\n    );\r\n  }\r\n}\r\n\r\nexport default Payment;","D:\\HocTap\\HK2_Nam4\\KLTN\\Project_Frontend\\electronicshop-client\\src\\components\\account\\ValidationForm.js",["727"],"const validatePassword = (password) => {\r\n  const pattern = /^(?=.*[a-z])(?=.*[A-Z])(?=.*\\d)(?=.*[#$@!%&*?])[A-Za-z\\d#$@!%&*?]{8,30}$/g\r\n  const check = pattern.exec(password);\r\n\r\n  if (check !== null) {\r\n    return \"\";\r\n  }\r\n\r\n  return `Mt khu cn phi c 8 n 30 k t, gm t nht 1 k t s, 1 k t thng, 1 k t hoa, 1 k t c bit`;\r\n}\r\n\r\nconst validateEmail = (email) => {\r\n  const pattern = /^[\\w-.]+@([\\w-]+\\.)+[\\w-]{2,4}$/g\r\n  const check = pattern.exec(email);\r\n\r\n  if (check !== null) {\r\n    return \"\";\r\n  }\r\n\r\n  return `Nhp ng nh dng Email (v d : abc@gmail.com)`;\r\n}\r\n\r\nconst validateUserName = (userName) => {\r\n  if (userName === \"\") {\r\n    return `Khng c b trng.`;\r\n  }\r\n\r\n  return \"\";\r\n}\r\n\r\nconst validateConfirmPassword = (password, confirmPassword) => {\r\n  if (password !== confirmPassword) {\r\n    return `Xc nhn mt khu khng khp vi mt khu hin ti`;\r\n  }\r\n\r\n  return \"\";\r\n}\r\n\r\nconst validatePhoneNumber = (phone) => {\r\n  const pattern = /^[0-9]{10}$/g\r\n  const check = pattern.exec(phone)\r\n  if (check !== null) {\r\n    return \"\";\r\n  }\r\n  return `Qu khch vui lng nhp s in thoi gm 10 ch s.`;\r\n}\r\n\r\nconst validateString = (str) => {\r\n  const pattern = /^[^`~!/@#$%^&*_+={}\\[\\]|\\\\:;<,>.?]*$/g\r\n  const check = pattern.exec(str)\r\n  if(check!==null){\r\n    return \"\";\r\n  }\r\n  return `Vui lng khng nhp cc k t c bit`;\r\n}\r\n\r\nexport { validatePassword, validateEmail, validateUserName, validateConfirmPassword, validatePhoneNumber, validateString };","D:\\HocTap\\HK2_Nam4\\KLTN\\Project_Frontend\\electronicshop-client\\src\\components\\homes\\CardItem.js",["728","729"],"import React, { Component } from 'react';\r\nimport \"./CardItem.css\";\r\nimport CurrencyFormat from 'react-currency-format';\r\nimport { ContextApi } from '../../contexts/Context';\r\nimport StarRatings from 'react-star-ratings';\r\nimport { totalRate } from '../../api/reviewApi';\r\n\r\nclass CardItem extends Component {\r\n\r\n  constructor(props) {\r\n    super(props);\r\n    this.state = {\r\n      rating: 0\r\n    }\r\n  }\r\n\r\n  async componentDidMount() {\r\n    const restotalRating = await totalRate(this.props.product.id);\r\n    this.setSt